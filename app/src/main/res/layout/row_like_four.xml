<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="likeFourRowModel" type="com.chhurngsapplication.app.modules.likefour.data.model.LikeFourRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowellipsefortythree"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <FrameLayout android:id="@+id/frameStackellipsefortythree"
            android:layout_height="@dimen/_52pxv" android:layout_width="@dimen/_54pxh">
            <ImageView android:id="@+id/imageEllipseFortyThree"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_46pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="@dimen/_46pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_ellipse43_1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse43_1"/>
            <ImageView android:id="@+id/imageLocation"
                android:layout_gravity="bottom|end"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_23pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_location_213"
                tools:ignore="ContentDescription" tools:src="@drawable/img_location_213"/>
        </FrameLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtAnandaVia"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_26pxv"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_weight="0.59" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{likeFourRowModel.txtAnandaVia}"
            style="@style/txtMontserratbold13" tools:text="@string/lbl_ananda_via"/>
    </LinearLayout>
</layout>
