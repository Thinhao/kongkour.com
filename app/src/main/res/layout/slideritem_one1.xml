<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="imageSliderSliderlanguageModel" type="com.chhurngsapplication.app.modules.one.data.model.ImageSliderSliderlanguageModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowlanguage"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/_335pxh"
        android:layout_marginStart="@dimen/_335pxh"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylegreen_A700cornerRadius">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnlanguage"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_weight="0.48" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtLanguage"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_9pxh"
                android:singleLine="true"
                android:text="@{imageSliderSliderlanguageModel.txtLanguage}"
                style="@style/txtPoppinssemibold18" tools:text="@string/lbl_graphic_design4"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCoursesCounter"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_7pxh"
                android:singleLine="true"
                android:text="@{imageSliderSliderlanguageModel.txtCoursesCounter}"
                style="@style/txtPoppinsregular14" tools:text="@string/lbl_26_courses"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowstar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageStar"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star_white_a700"
                    imageUrl="@{imageSliderSliderlanguageModel.imageStar}"
                    placeHolder="@{@drawable/img_star_white_a700}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_star_white_a700"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFortyFive"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.22"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_7pxh"
                    android:singleLine="true"
                    android:text="@{imageSliderSliderlanguageModel.txtFortyFive}"
                    style="@style/txtPoppinsregular14" tools:text="@string/lbl_4_5"/>
                <ImageView android:id="@+id/imageClock"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_clock"
                    imageUrl="@{imageSliderSliderlanguageModel.imageClock}"
                    placeHolder="@{@drawable/img_clock}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_clock"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_weight="0.46"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_7pxh"
                    android:singleLine="true"
                    android:text="@{imageSliderSliderlanguageModel.txtTime}"
                    style="@style/txtPoppinsregular14" tools:text="@string/lbl_24min"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/framePersons"
                android:layout_height="@dimen/_40pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_20pxv" android:layout_width="@dimen/_130pxh">
                <ImageView android:id="@+id/imagePlaceholder"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageUrl="@{imageSliderSliderlanguageModel.imagePlaceholder}"
                    placeHolder="@{@drawable/image_not_found}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_placeholder"/>
                <ImageView android:id="@+id/imagePlaceholderOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageUrl="@{imageSliderSliderlanguageModel.imagePlaceholderOne}"
                    placeHolder="@{@drawable/image_not_found}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_placeholder_40x40"/>
                <ImageView android:id="@+id/imagePlaceholderTwo"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageUrl="@{imageSliderSliderlanguageModel.imagePlaceholderTwo}"
                    placeHolder="@{@drawable/image_not_found}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_placeholder_1"/>
                <ImageView android:id="@+id/imagePlaceholderThree"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageUrl="@{imageSliderSliderlanguageModel.imagePlaceholderThree}"
                    placeHolder="@{@drawable/image_not_found}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_placeholder_2"/>
            </FrameLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameStackgrouptwo"
            android:layout_height="@dimen/_154pxv"
            android:layout_marginTop="@dimen/_29pxv" android:layout_width="@dimen/_154pxh">
            <ImageView android:id="@+id/imageGroupTwo"
                android:layout_gravity="center"
                android:layout_height="@dimen/_154pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_width="@dimen/_149pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_group2"
                imageUrl="@{imageSliderSliderlanguageModel.imageGroupTwo}"
                placeHolder="@{@drawable/img_group2}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group2"/>
            <TextView android:ellipsize="end"
                android:gravity="top|start"
                android:id="@+id/txt459Joined"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_29pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_7pxh"
                android:singleLine="true"
                android:text="@{imageSliderSliderlanguageModel.txt459Joined}"
                style="@style/txtPoppinsregular14" tools:text="@string/lbl_459_joined"/>
        </FrameLayout>
    </LinearLayout>
</layout>
