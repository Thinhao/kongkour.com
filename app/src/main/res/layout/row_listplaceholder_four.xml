<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listplaceholderFourRowModel" type="com.chhurngsapplication.app.modules.one.data.model.ListplaceholderFourRowModel"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnplaceholderFour"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:orientation="vertical" style="@style/groupStylewhite_3">
        <FrameLayout android:id="@+id/frameStackplaceholderFour"
            android:layout_gravity="start"
            android:layout_height="@dimen/_160pxv" android:layout_width="@dimen/_263pxh">
            <ImageView android:id="@+id/imagePlaceholderFour"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_135pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="@dimen/_263pxh"
                android:scaleType="fitXY" cornerRadius="@{@dimen/_2pxh}"
                imageRes="@{@drawable/img_placeholder_135x263}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_placeholder_135x263"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguageTwo"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_6pxh"
                android:singleLine="true"
                android:text="@{listplaceholderFourRowModel.txtLanguageTwo}"
                style="@style/txtPoppinsmedium12" tools:text="@string/lbl_design2"/>
            <ImageButton
                android:background="@drawable/rectangle_bg_blue_900_radius_24"
                android:id="@+id/btnVector"
                android:layout_gravity="bottom|end"
                android:layout_height="@dimen/_48pxh"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_48pxh"
                android:paddingHorizontal="@dimen/_16pxh"
                android:paddingVertical="@dimen/_16pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_vector" tools:ignore="ContentDescription"/>
        </FrameLayout>
        <TextView android:ellipsize="end" android:gravity="start"
            android:id="@+id/txtLanguageOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_8pxh"
            android:singleLine="true"
            android:text="@{listplaceholderFourRowModel.txtLanguageOne}"
            style="@style/txtPoppinsmedium16" tools:text="@string/msg_what_is_graphic"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearCount"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_19pxv"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageClockOne"
                android:layout_height="@dimen/_13pxh"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="@dimen/_13pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_clock_gray_600"
                tools:ignore="ContentDescription" tools:src="@drawable/img_clock_gray_600"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtContentCounter"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_weight="0.42" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_7pxh"
                android:singleLine="true"
                android:text="@{listplaceholderFourRowModel.txtContentCounter}"
                style="@style/txtPoppinsregular14_1" tools:text="@string/lbl_26_content"/>
            <ImageView android:id="@+id/imageTicket"
                android:layout_height="@dimen/_15pxh"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="@dimen/_96pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_ticket_orange_600"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ticket_orange_600"/>
        </LinearLayout>
    </LinearLayout>
</layout>
