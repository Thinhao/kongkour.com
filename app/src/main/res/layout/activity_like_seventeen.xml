<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="likeSeventeenVM" type="com.chhurngsapplication.app.modules.likeseventeen.data.viewmodel.LikeSeventeenVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnrectanglefive"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_33pxv"
        android:layout_marginEnd="@dimen/_30pxh"
        android:layout_marginTop="@dimen/_21pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_900">
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearLikeSeventeen"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearRowrectanglefive"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <View
                        android:background="@drawable/rectangle_bg_white_a700_radius_tl_12_tr_12"
                        android:id="@+id/viewRectangleFive"
                        android:layout_height="@dimen/_49pxv" android:layout_width="@dimen/_105pxh"/>
                    <ImageView android:id="@+id/imageOverflowmenu"
                        android:layout_height="@dimen/_29pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_272pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="@dimen/_7pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_overflowmenu"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_overflowmenu"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowtime"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_3pxh"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_marginTop="@dimen/_85pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowmenu"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.66"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMenu"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_27pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_menu_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_menu_white_a700"/>
                        <ImageView android:id="@+id/imageSend"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_31pxh"
                            android:layout_width="@dimen/_27pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_send_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_send_white_a700"/>
                        <ImageView android:id="@+id/imageContrast"
                            android:layout_height="@dimen/_23pxh"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_26pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="@dimen/_26pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_contrast_10"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_contrast_10"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_134pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="0.34"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{likeSeventeenVM.likeSeventeenModel.txtTime}"
                        style="@style/txtContent9_1" tools:text="@string/msg_today_at_9_00_a"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearInfolikescomm"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_36pxh"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLanguage"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.39"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{likeSeventeenVM.likeSeventeenModel.txtLanguage}"
                        style="@style/txtMontserratbold12_1" tools:text="@string/lbl_7_9k_like2"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLanguageOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_28pxh"
                        android:layout_weight="0.61"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{likeSeventeenVM.likeSeventeenModel.txtLanguageOne}"
                        style="@style/txtMontserratbold12_1" tools:text="@string/lbl_100_comment2"/>
                </LinearLayout>
                <TextView android:gravity="end"
                    android:id="@+id/txtLanguageTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_width="match_parent"
                    android:text="@{likeSeventeenVM.likeSeventeenModel.txtLanguageTwo}"
                    style="@style/txtMontserratromanmedium15_1" tools:text="@string/msg_hi_guys_this_i"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLike"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_58pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{likeSeventeenVM.likeSeventeenModel.txtLike}"
                    style="@style/txtMontserratbold14" tools:text="@string/lbl_like"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="start"
                    android:id="@+id/recyclerListellipsefortythree"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_36pxv"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_27pxh}" tools:itemCount="10" tools:listitem="@layout/row_listellipsefortythree"/>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
</layout>
