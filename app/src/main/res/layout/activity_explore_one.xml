<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="exploreOneVM" type="com.chhurngsapplication.app.modules.exploreone.data.viewmodel.ExploreOneVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/_29pxh"
        android:layout_marginStart="@dimen/_22pxh"
        android:layout_marginTop="@dimen/_47pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_900">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_22"
                    android:id="@+id/btnArrowleft"
                    android:layout_height="@dimen/_44pxh"
                    android:layout_width="@dimen/_44pxh"
                    android:paddingHorizontal="@dimen/_13pxh"
                    android:paddingVertical="@dimen/_13pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_arrowleft_gray_926" tools:ignore="ContentDescription"/>
                <FrameLayout android:id="@+id/frameNotification"
                    android:layout_height="@dimen/_34pxv"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_283pxh"
                    android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_21pxh">
                    <View
                        android:background="@drawable/rectangle_border_white_a700_radius_10_955"
                        android:id="@+id/viewEllipseSixtyNine"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_21pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginTop="@dimen/_4pxv" android:layout_width="@dimen/_21pxh"/>
                    <ProgressBar android:id="@+id/progressBarGroup185"
                        android:indeterminate="false"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:max="100"
                        android:minHeight="@dimen/_14pxv"
                        android:progress="93"
                        android:progressDrawable="@drawable/layer_list_border_white_a700" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                    <View
                        android:background="@drawable/rectangle_bg_white_a700_radius_2_645"
                        android:id="@+id/viewEllipseSeventy"
                        android:layout_gravity="top|center"
                        android:layout_height="@dimen/_5pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_8pxh"
                        android:layout_marginStart="@dimen/_7pxh" android:layout_width="@dimen/_5pxh"/>
                    <View
                        android:background="@drawable/rectangle_bg_white_a700_radius_2_645"
                        android:id="@+id/viewEllipseSeventyOne"
                        android:layout_gravity="bottom|center"
                        android:layout_height="@dimen/_5pxh"
                        android:layout_marginEnd="@dimen/_7pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_5pxh"/>
                    <ImageView android:id="@+id/imageVectorSeven"
                        android:layout_gravity="top|end"
                        android:layout_height="@dimen/_6pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_4pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="@dimen/_6pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector7"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector7"/>
                </FrameLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_41pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnsearchform"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.SearchView
                    android:background="@drawable/rectangle_border_white_a700"
                    android:gravity="center_horizontal"
                    android:id="@+id/searchViewSearchform"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_width="match_parent"
                    android:theme="@style/searchViewsearchform"
                    app:iconifiedByDefault="false"
                    app:queryBackground="@android:color/transparent"
                    app:queryHint="@string/lbl18" app:searchIcon="@drawable/img_search_white_a700"/>
                <LinearLayout android:id="@+id/linearRowsixtyone"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_48pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSixtyOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_626pxv"
                        android:layout_weight="0.07"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{exploreOneVM.exploreOneModel.txtSixtyOne}"
                        style="@style/txtContent18" tools:text="@string/lbl34"/>
                    <LinearLayout android:id="@+id/linearRow"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.93"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumn"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:layout_width="0dp" android:orientation="vertical">
                            <ImageView
                                android:id="@+id/imageImageprofile"
                                android:layout_height="@dimen/_213pxh"
                                android:layout_width="match_parent"
                                android:scaleType="fitXY"
                                imageRes="@{@drawable/img_imageprofile_54}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_imageprofile_54"/>
                            <ImageView
                                android:id="@+id/imageImageprofileOne"
                                android:layout_height="@dimen/_128pxh"
                                android:layout_marginTop="@dimen/_9pxv"
                                android:layout_width="match_parent"
                                android:scaleType="fitXY"
                                imageRes="@{@drawable/img_imageprofile_55}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_imageprofile_55"/>
                            <ImageView
                                android:id="@+id/imageImageprofileTwo"
                                android:layout_height="@dimen/_213pxh"
                                android:layout_marginTop="@dimen/_9pxv"
                                android:layout_width="match_parent"
                                android:scaleType="fitXY"
                                imageRes="@{@drawable/img_imageprofile_56}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_imageprofile_56"/>
                            <ImageView
                                android:id="@+id/imageImageprofileThree"
                                android:layout_height="@dimen/_128pxh"
                                android:layout_marginTop="@dimen/_9pxv"
                                android:layout_width="match_parent"
                                android:scaleType="fitXY"
                                imageRes="@{@drawable/img_imageprofile_57}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_imageprofile_57"/>
                        </LinearLayout>
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/recyclerExploreOne"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_weight="0.5"
                            android:layout_width="0dp"
                            android:nestedScrollingEnabled="false"
                            android:orientation="vertical"
                            app:layoutManager="androidx.recyclerview.widget.StaggeredGridLayoutManager"
                            app:spanCount="1"
                            itemSpace="@{@dimen/_150pxh}"
                            tools:itemCount="4" tools:listitem="@layout/row_explore_one"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
</layout>
