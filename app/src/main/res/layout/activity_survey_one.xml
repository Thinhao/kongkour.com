<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="surveyOneVM" type="com.chhurngsapplication.app.modules.surveyone.data.viewmodel.SurveyOneVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackrectangle901"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameSurveyOne"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylewhite_A700">
            <ImageView android:id="@+id/imageRectangle901"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_rectangle901_816x375}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle901_816x375"/>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerSurveyOne"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_192pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_192pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.StaggeredGridLayoutManager"
                app:spanCount="2" itemSpace="@{@dimen/_15pxh}"
                tools:itemCount="4" tools:listitem="@layout/row_survey_one"/>
            <LinearLayout android:id="@+id/linearColumnlanguage"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_40pxv"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="wrap_content" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearRowlanguage"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLanguage"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.46"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_10pxh"
                        android:singleLine="true"
                        android:text="@{surveyOneVM.surveyOneModel.txtLanguage}"
                        style="@style/txt_2" tools:text="@string/lbl_e_learning"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPhotography"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_93pxh"
                        android:layout_weight="0.54"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_10pxh"
                        android:singleLine="true"
                        android:text="@{surveyOneVM.surveyOneModel.txtPhotography}"
                        style="@style/txt_2" tools:text="@string/lbl_photography"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameStackrectangle897"
                    android:layout_height="@dimen/_123pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="@dimen/_163pxh" style="@style/groupStyleblue_400cornerRadius">
                    <ImageView android:id="@+id/imageRectangle897"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_120pxv"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_12pxh}"
                        imageRes="@{@drawable/img_rectangle897}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle897"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguageOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_10pxh"
                    android:singleLine="true"
                    android:text="@{surveyOneVM.surveyOneModel.txtLanguageOne}"
                    style="@style/txt_2" tools:text="@string/msg_digital_marketi"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnContinue"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_84pxh"
                    android:layout_marginTop="@dimen/_37pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_continue"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_7" tools:text="@string/lbl_continue"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguageTwo"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_330pxv"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_330pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_10pxh"
                android:singleLine="true"
                android:text="@{surveyOneVM.surveyOneModel.txtLanguageTwo}"
                style="@style/txt_2" tools:text="@string/lbl_graphic_design"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguageThree"
                android:layout_gravity="top|end"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_330pxv"
                android:layout_marginEnd="@dimen/_109pxh"
                android:layout_marginStart="@dimen/_109pxh"
                android:layout_marginTop="@dimen/_330pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_10pxh"
                android:singleLine="true"
                android:text="@{surveyOneVM.surveyOneModel.txtLanguageThree}"
                style="@style/txt_2" tools:text="@string/lbl_branding"/>
            <LinearLayout android:id="@+id/linearColumngroup189"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_45pxv"
                android:layout_marginEnd="@dimen/_51pxh"
                android:layout_marginStart="@dimen/_51pxh"
                android:layout_marginTop="@dimen/_45pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageGroup189"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_marginEnd="@dimen/_86pxh"
                    android:layout_marginStart="@dimen/_96pxh"
                    android:layout_width="@dimen/_78pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group189_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group189_black_900"/>
                <TextView android:gravity="start"
                    android:id="@+id/txtLanguageFour"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:text="@{surveyOneVM.surveyOneModel.txtLanguageFour}"
                    style="@style/txtManropeextrabold20" tools:text="@string/msg_what_creative_f"/>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
