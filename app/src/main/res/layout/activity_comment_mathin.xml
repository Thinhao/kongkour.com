<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="commentMathinVM" type="com.chhurngsapplication.app.modules.commentmathin.data.viewmodel.CommentMathinVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnrectanglefive"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_117pxv"
        android:layout_marginEnd="@dimen/_30pxh"
        android:layout_marginTop="@dimen/_21pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_900">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="end"
                android:id="@+id/linearCommentMathi"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearRowrectanglefive"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <View
                        android:background="@drawable/rectangle_bg_white_a700_radius_tl_12_tr_12"
                        android:id="@+id/viewRectangleFive"
                        android:layout_height="@dimen/_49pxv" android:layout_width="@dimen/_105pxh"/>
                    <ImageView android:id="@+id/imageOverflowmenu"
                        android:layout_height="@dimen/_29pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_272pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="@dimen/_7pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_overflowmenu"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_overflowmenu"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_75pxh"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_476pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{commentMathinVM.commentMathinModel.txtLanguage}"
                    style="@style/txtMontserratromanregular15_1" tools:text="@string/msg_poster_design_f"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnlanguageOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_33pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLanguageOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{commentMathinVM.commentMathinModel.txtLanguageOne}"
                        style="@style/txtMontserratbold14" tools:text="@string/lbl_comments"/>
                    <LinearLayout
                        android:id="@+id/linearRowellipsefortythree"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_35pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/imageEllipseFortyThree"
                            android:layout_height="@dimen/_46pxh"
                            android:layout_marginBottom="@dimen/_114pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_46pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_ellipse43}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse43"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnanandavia"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_21pxh"
                            android:layout_weight="0.86"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtAnandaVia"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{commentMathinVM.commentMathinModel.txtAnandaVia}"
                                style="@style/txtMontserratbold13" tools:text="@string/lbl_ananda_via"/>
                            <TextView
                                android:gravity="center_horizontal"
                                android:id="@+id/txtDescription"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_1pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="match_parent"
                                android:text="@{commentMathinVM.commentMathinModel.txtDescription}"
                                style="@style/txtMontserratregular10" tools:text="@string/msg_the_poster_des"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout android:gravity="end"
                        android:id="@+id/linearRowsend"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageSend"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_send_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_send_white_a700"/>
                        <ImageView android:id="@+id/imageLocation"
                            android:layout_height="@dimen/_17pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_19pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_location_74"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_location_74"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearRowellipsefortythreeOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/imageEllipseFortyThreeOne"
                            android:layout_height="@dimen/_46pxh"
                            android:layout_marginBottom="@dimen/_148pxv"
                            android:layout_width="@dimen/_46pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_ellipse43_46x46}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse43_46x46"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnsusansanae"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_22pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.86"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtSusansanae"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{commentMathinVM.commentMathinModel.txtSusansanae}"
                                style="@style/txtMontserratbold13" tools:text="@string/lbl_susan_sanae"/>
                            <TextView
                                android:id="@+id/txtDescriptionOne"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="match_parent"
                                android:text="@{commentMathinVM.commentMathinModel.txtDescriptionOne}"
                                style="@style/txtMontserratregular10" tools:text="@string/msg_the_poster_des2"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout android:gravity="end"
                        android:id="@+id/linearRowcheckmark"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageCheckmark"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_checkmark_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark_white_a700"/>
                        <ImageView android:id="@+id/imageLocationOne"
                            android:layout_height="@dimen/_17pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_19pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_location_75"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_location_75"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
