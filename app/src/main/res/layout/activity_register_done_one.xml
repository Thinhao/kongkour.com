<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="registerDoneOneVM" type="com.chhurngsapplication.app.modules.registerdoneone.data.viewmodel.RegisterDoneOneVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearRegisterdone"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle_1">
        <FrameLayout android:id="@+id/frameStackmaskgroup"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_128pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_160pxv"
            android:layout_width="@dimen/_128pxh" style="@style/groupStylewhite_A700cornerRadius">
            <ImageView android:id="@+id/imageMaskgroup"
                android:layout_gravity="start"
                android:layout_height="@dimen/_128pxh"
                android:layout_width="@dimen/_128pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_maskgroup"
                tools:ignore="ContentDescription" tools:src="@drawable/img_maskgroup"/>
            <LinearLayout android:id="@+id/linearColumncheckmark"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_23pxv"
                android:layout_marginEnd="@dimen/_32pxh"
                android:layout_marginStart="@dimen/_32pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageCheckmark"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_33pxh"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_width="@dimen/_33pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_checkmark"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark"/>
                <View
                    android:background="@drawable/rectangle_bg_white_a700_radius_3"
                    android:id="@+id/viewRectangleSeven"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_6pxv"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_7pxv" android:layout_width="@dimen/_45pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_white_a700_radius_4"
                    android:id="@+id/lineRectangleEight"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_6pxv"
                    android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_63pxh"/>
            </LinearLayout>
        </FrameLayout>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtYouraccountha"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_35pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_12pxh"
            android:text="@{registerDoneOneVM.registerDoneOneModel.txtYouraccountha}"
            style="@style/txtManropeextrabold28" tools:text="@string/msg_your_account_ha"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumn"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_294pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecornerRadius">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumngetstarted"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_1">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtGetStarted"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_116pxh"
                    android:layout_marginStart="@dimen/_117pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@{registerDoneOneVM.registerDoneOneModel.txtGetStarted}"
                    style="@style/txtPoppinsbold16" tools:text="@string/lbl_get_started"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLogin"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginEnd="@dimen/_117pxh"
                    android:layout_marginStart="@dimen/_117pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{registerDoneOneVM.registerDoneOneModel.txtLogin}"
                    style="@style/txtManropesemibold16" tools:text="@string/lbl_login"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
