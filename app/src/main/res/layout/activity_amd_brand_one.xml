<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="amdBrandOneVM" type="com.chhurngsapplication.app.modules.amdbrandone.data.viewmodel.AmdBrandOneVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnuntitled1zthree"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_52pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearAMDBrandOne"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:gravity="start"
                    android:id="@+id/frameStack"
                    android:layout_height="@dimen/_142pxv"
                    android:layout_width="match_parent" style="@style/groupStyle_2">
                    <FrameLayout
                        android:id="@+id/frameStackuntitled1zthree"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_142pxv"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_width="@dimen/_412pxh" style="@style/groupStyleorange_600">
                        <ImageView
                            android:id="@+id/imageUntitled1zThree"
                            android:layout_gravity="top|center"
                            android:layout_height="@dimen/_135pxh"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_128pxh"
                            android:layout_marginStart="@dimen/_128pxh"
                            android:layout_width="@dimen/_154pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_untitled1z3}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_untitled1z3"/>
                    </FrameLayout>
                    <FrameLayout android:id="@+id/frameStackarrowleft"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_142pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="@dimen/_48pxh" style="@style/groupStyleorange_A700cornerRadius">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_17pxh"
                            android:layout_marginBottom="@dimen/_40pxv"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_40pxv"
                            android:layout_width="@dimen/_13pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    </FrameLayout>
                </FrameLayout>
                <ImageView android:gravity="center_horizontal"
                    android:id="@+id/imagePLACEYOURDESI"
                    android:layout_height="@dimen/_235pxh"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_27pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_12pxh}"
                    imageRes="@{@drawable/img_placeyourdesi_235x394}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_placeyourdesi_235x394"/>
                <TextView android:id="@+id/txtPrice"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="wrap_content"
                    android:text="@{amdBrandOneVM.amdBrandOneModel.txtPrice}"
                    style="@style/txtPlusJakartaSansromanbold13" tools:text="@string/msg_amd_20"/>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtPriceOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_33pxh"
                    android:layout_marginStart="@dimen/_33pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{amdBrandOneVM.amdBrandOneModel.txtPriceOne}"
                    style="@style/txtPlusJakartaSansromanbold20" tools:text="@string/msg_amd_brand_guide"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtThePurposeof"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_33pxh"
                    android:layout_marginStart="@dimen/_33pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{amdBrandOneVM.amdBrandOneModel.txtThePurposeof}"
                    style="@style/txtPlusJakartaSansromanbold14" tools:text="@string/msg_the_purpose_of"/>
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="match_parent"
                    android:text="@{amdBrandOneVM.amdBrandOneModel.txtDescription}"
                    style="@style/txtPlusJakartaSansromanregular12" tools:text="@string/msg_these_guideline"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowstar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_34pxh"
                    android:layout_marginStart="@dimen/_34pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageStar"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt5046reviews"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_weight="0.87"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{amdBrandOneVM.amdBrandOneModel.txt5046reviews}"
                        style="@style/txtPlusJakartaSansromanregular10_1" tools:text="@string/msg_5_0_46_reviews"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btn"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_53pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_58pxv"
                    android:layout_width="@dimen/_348pxh"
                    android:paddingBottom="@dimen/_11pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_11pxv"
                    android:singleLine="true"
                    android:text="@string/msg15"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_4" tools:text="@string/msg15"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
