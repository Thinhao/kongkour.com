<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="mcdonaldSBrandOneVM" type="com.chhurngsapplication.app.modules.mcdonaldsbrandone.data.viewmodel.McdonaldSBrandOneVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnplaceyourdesi"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_36pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearMcDonaldsBran"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle_3">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowarrowleft"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStyleorange_600">
                        <FrameLayout
                            android:id="@+id/frameStackarrowleft"
                            android:layout_height="@dimen/_142pxv"
                            android:layout_width="@dimen/_50pxh" style="@style/groupStyleorange_A700cornerRadius">
                            <ImageView android:id="@+id/imageArrowleft"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_17pxh"
                                android:layout_marginBottom="@dimen/_40pxv"
                                android:layout_marginEnd="@dimen/_18pxh"
                                android:layout_marginStart="@dimen/_18pxh"
                                android:layout_marginTop="@dimen/_40pxv"
                                android:layout_width="@dimen/_13pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_arrowleft"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                        </FrameLayout>
                        <FrameLayout
                            android:id="@+id/frameStackbrandguideline"
                            android:layout_height="@dimen/_118pxv"
                            android:layout_marginBottom="@dimen/_16pxv"
                            android:layout_marginEnd="@dimen/_122pxh"
                            android:layout_marginStart="@dimen/_83pxh"
                            android:layout_marginTop="@dimen/_6pxv" android:layout_width="@dimen/_158pxh">
                            <TextView android:ellipsize="end"
                                android:gravity="top|start"
                                android:id="@+id/txtBrandGuideline"
                                android:layout_gravity="bottom|start"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{mcdonaldSBrandOneVM.mcdonaldSBrandOneModel.txtBrandGuideline}"
                                style="@style/txtPoppinsbold18" tools:text="@string/msg_brand_guideline3"/>
                            <ImageView
                                android:id="@+id/imageLayerTwelve"
                                android:layout_gravity="end"
                                android:layout_height="@dimen/_116pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginEnd="@dimen/_11pxh"
                                android:layout_marginStart="@dimen/_11pxh"
                                android:layout_width="@dimen/_126pxh"
                                android:scaleType="fitXY"
                                imageRes="@{@drawable/img_layer11}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_layer11"/>
                        </FrameLayout>
                    </LinearLayout>
                </LinearLayout>
                <ImageView android:gravity="center_horizontal"
                    android:id="@+id/imagePLACEYOURDESI"
                    android:layout_height="@dimen/_214pxh"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_12pxh}"
                    imageRes="@{@drawable/img_placeyourdesi_214x385}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_placeyourdesi_214x385"/>
                <TextView android:id="@+id/txtMcDonalds2019"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="wrap_content"
                    android:text="@{mcdonaldSBrandOneVM.mcdonaldSBrandOneModel.txtMcDonalds2019}"
                    style="@style/txtPlusJakartaSansromanbold13" tools:text="@string/lbl_mcdonald_s_2019"/>
                <TextView android:id="@+id/txtLanguage"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_31pxv"
                    android:layout_width="wrap_content"
                    android:text="@{mcdonaldSBrandOneVM.mcdonaldSBrandOneModel.txtLanguage}"
                    style="@style/txtPlusJakartaSansromanbold20" tools:text="@string/msg_mcdonald_s_bra"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtThePurposeof"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{mcdonaldSBrandOneVM.mcdonaldSBrandOneModel.txtThePurposeof}"
                    style="@style/txtPlusJakartaSansromanbold14" tools:text="@string/msg_the_purpose_of"/>
                <TextView android:gravity="start"
                    android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="match_parent"
                    android:text="@{mcdonaldSBrandOneVM.mcdonaldSBrandOneModel.txtDescription}"
                    style="@style/txtPlusJakartaSansromanregular12" tools:text="@string/msg_mcdonald_s_is_o"/>
                <TextView android:gravity="start"
                    android:id="@+id/txtDescriptionOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:text="@{mcdonaldSBrandOneVM.mcdonaldSBrandOneModel.txtDescriptionOne}"
                    style="@style/txtPlusJakartaSansromanregular12" tools:text="@string/msg_directed_by_tyl"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowstar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageStar"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt5046reviews"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_weight="0.87"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{mcdonaldSBrandOneVM.mcdonaldSBrandOneModel.txt5046reviews}"
                        style="@style/txtPlusJakartaSansromanregular10_1" tools:text="@string/msg_5_0_46_reviews"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnBrandGuidelinesOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_48pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_14pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_14pxv"
                    android:singleLine="true"
                    android:text="@string/msg_brand_guideline3"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_9" tools:text="@string/msg_brand_guideline3"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
